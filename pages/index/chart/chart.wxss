.chart1 {
  width: 100%;
height: 200px;
background: #F5F6FA;
border-radius: 3px;
margin: 0;
}
.canvas {
  width: 100%;
  height: 250px;
}
.page-title {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  margin-top: 64rpx;
}
.page-title text {
font-size: 16px;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #333333;
line-height: 22px;
}
.date-btns {
  display: flex;
  width: 92%;
  margin: 60rpx auto;
  height: 29px;
background: #F5F6FA;
border-radius: 3px;
border: 1px solid #EDEFF5;
}
.week-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50%;
  
}
.month-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50%;
  background: #fff;
}
.chart-title {
font-size: 15px;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #333333;
line-height: 21px;
margin-left: 15px;
margin-bottom: 15px;
}
.chart-title2 {
  font-size: 15px;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #333333;
line-height: 21px;
margin-left: 15px;
margin-bottom: 15px;
margin-top: 27px;
}
.chart-dates {
  width: 100%;
  display: flex;
  justify-content: space-around;
  padding-top: 8px;
}
.chart-dates text {
font-size: 12px;
font-family: PingFangSC-Medium, PingFang SC;
font-weight: 500;
color: #999999;
line-height: 17px;
}