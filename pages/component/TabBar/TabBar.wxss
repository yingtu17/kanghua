.tabbar {
  width: 100%;
height: 152rpx;
background: #FFFFFF;
box-shadow: 0px -1px 0px 0px rgba(0, 0, 0, 0.05);
position: fixed;
bottom: 0;
left: 0;
display: flex;
justify-content: space-around;
}
.tab-img {
  width: 22px;
  height: 22px;
}
.tab-text {
  font-size: 10px;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #ABB0C1;
  line-height: 14px;
}
.index-tab, .msg-tab, .me-tab {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-top: 6px;
}
.tab-active {
font-size: 10px;
font-family: PingFangSC-Regular, PingFang SC;
font-weight: 400;
color: #008385;
line-height: 14px;
}
.navigator {
  height: 44px;
}